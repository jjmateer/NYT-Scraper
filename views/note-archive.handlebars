{{> jumbotron/jumbotron-block}}
<h1 id="mainHeader">Note Archive</h1>
{{#unless data}}
<form id="noteArchiveForm">
    <h3>Search previously deleted notes</h3>
    <label for="archiveID" class="col-form-label">Article ID:</label>
    <input required type="text" id="archiveID" name="archiveid">
    <input class="btn btn-danger" id="archiveBtn" type="submit" value="Submit">
</form>
{{/unless}}
<div>
    {{#if data}}
    {{#each data}}
    {{> archive-card/archive-card-block}}
    {{/each}}
    <a id="archiveBack" class="btn btn-danger btn-lg" href="/notearchive" role="button">Go back</a>
    {{/if}}
</div>